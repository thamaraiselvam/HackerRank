{
	"name": "hackerrank",
	"version": "1.0.0",
	"description": "Solutions for HackerRank Problems in Javascript.",
	"main": "index.js",
	"scripts": {
		"test": "mocha --recursive",
		"report": "nyc mocha --recursive",
