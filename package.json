		"eslint": "7.32.0",
		"eslint-config-airbnb-base": "14.2.1",
		"eslint-plugin-import": "2.24.2",
		"husky": "7.0.2",
		"lint-staged": "11.2.1",
		"nyc": "^15.1.0",
		"prettier": "2.4.1"
	}
}
